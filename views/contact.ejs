<%- include("partials/header"); -%>


    <!-- Form for the user to enter contact info, then console.log the results -->
    <section id="user-form">
        <div class="container">
            <h2 class="mt-3">Want to send me a message? Fill out this form!</h2>
            <form method="POST" class="border rounded-3 shadow-lg p-3">
                <div class="mb-3 mt-5">
                    <label class="form-label">First Name</label>
                    <input type="text" name="fName" required class="form-control">
                </div>

                <div class="mb-3 mt-5">
                    <label class="form-label">Last Name</label>
                    <input type="text" name="lName" required class="form-control">
                </div>

                <div class="mb-3 mt-5">
                    <label class="form-label">Phone Number</label>
                    <input type="number" name="phone" required class="form-control">
                </div>

                <div class="mb-3 mt-5">
                    <label class="form-label">Email</label>
                    <input type="email" name="email" required class="form-control">
                </div>

                <div class="mb-3 mt-5">
                    <label class="form-label">Password</label>
                    <input type="password" name="password" required class="form-control">
                </div>

                <button type="submit" class="btn mb-4 btn-primary">Submit</button>
            </form>
        </div>
    </section>



    <%- include("partials/footer"); -%>